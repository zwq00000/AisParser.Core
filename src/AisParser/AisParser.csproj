<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net40;net45;netstandard2.0</TargetFrameworks>
    <AssemblyName>AisParser.Core</AssemblyName>
    <RootNamespace>AisParser</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>ZhaoWeiQiang&lt;zwq00000@gmail.com&gt;</Authors>
    <PackageId>AisParser.Core</PackageId>
    <Product>AIS Parser SDK for .Net Core</Product>
    <Description>source from https://github.com/bcl/aisparser ,add .net stadard2.0 version</Description>
    <Copyright>zwq00000@hotmail.com</Copyright>
    <PackageTags>AIS;NetStandard</PackageTags>
    <RepositoryType>git</RepositoryType>
    <Company>XiGe Tech CO.</Company>
    <RepositoryUrl>https://github.com/zwq00000/aisparser</RepositoryUrl>
  </PropertyGroup>
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <VersionPrefix>1.0.1</VersionPrefix>
    <VersionSuffix>alpha3</VersionSuffix>
  </PropertyGroup>
  
</Project>
